{
  "name": "@applitools/templatetemplate",
  "version": "1.0.0",
  "description": "",
  "main": "lib/src/templatetemplate.js",
  "types": "lib/src/templatetemplate.d.ts",
  "engines": {
    "node": ">=12.3.0"
  },
  "files": [
    "deployment",
    "lib/src",
    "lib/scripts",
    "src",
    "scripts"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "bin": {
    "run-templatetemplate": "scripts/run-templatetemplate.js"
  },
  "scripts": {
    "start": "lib/scripts/run-templatetemplate.js",
    "build": "tsc",
    "test": "npm run eslint && npm run test:mocha-parallel",
    "test:mocha": "mocha -r ts-node/register --no-timeouts --exit 'test/unit/*.test.?s' 'test/it/*.test.?s' 'test/e2e/*.test.?s'",
    "test:mocha-parallel": "mocha-parallel-tests -r ts-node/register --no-timeouts --exit 'test/unit/*.test.?s' 'test/it/*.test.?s' 'test/e2e/*.test.?s'",
    "eslint": "eslint 'test/**/*.?s' 'test/**/*.?s'",
    "postbuild": "npm run dump-dep-versions && npm run build:docker",
    "dump-dep-versions": "write-dep-versions-to-file deployment/dep-versions.yml",
    "build:docker": "docker build -t applitools/templatetemplate --build-arg NPM_FILE=`cat ~/.npmrc` . && docker tag applitools/templatetemplate applitools/templatetemplate:${npm_package_version}",
    "postpublish": "npm run publish:docker",
    "publish:docker": "docker push applitools/templatetemplate:${npm_package_version}"
  },
  "keywords": [
    "applitools",
    "nodejs",
    "javascript"
  ],
  "author": "Gil Tayar <gil.tayar@applitools.com>",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://github.com/applitools/rendering-grid",
    "type": "git"
  },
  "dependencies": {
    "@applitools/loggly-pino": "^3.1.3",
    "fastify": "^2.8.0"
  },
  "devDependencies": {
    "@applitools/build-commons": "^1.1.13",
    "@applitools/docker-compose-mocha": "^1.4.0",
    "@applitools/docker-compose-testkit": "^1.1.2",
    "@applitools/http-commons": "^2.3.12",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha-no-only": "^1.0.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^6.1.4",
    "mocha-parallel-tests": "^2.2.2",
    "prettier": "^1.17.0",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2"
  }
}
