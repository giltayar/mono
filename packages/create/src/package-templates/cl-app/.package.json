{
  "name": "@applitools/templatetemplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/templatetemplate.js",
  "engines": {
    "node": ">=10.3.0"
  },
  "files": [
    "helm",
    "src",
    "scripts"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "bin": {
    "run-templatetemplate": "scripts/run-templatetemplate.js"
  },
  "scripts": {
    "start": "scripts/run-templatetemplate.js",
    "test": "npm run eslint && npm run test:mocha-parallel",
    "test:mocha": "mocha --no-timeouts --trace-warnings 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "test:mocha-parallel": "mocha-parallel-tests --no-timeouts --trace-warnings 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "eslint": "eslint '**/*.js'",
    "build": "#",
    "postbuild": "npm run build:docker",
    "build:docker": "docker build -t applitools/templatetemplate --build-arg NPM_FILE=`cat ~/.npmrc` .",
    "postpublish": "npm run publish:docker",
    "publish:docker": "docker tag applitools/templatetemplate applitools/templatetemplate:${npm_package_version} && docker push applitools/templatetemplate:${npm_package_version} && docker push applitools/templatetemplate:latest"
  },
  "keywords": [
    "applitools",
    "testing",
    "browser",
    "grid",
    "visual-testing",
    "nodejs",
    "javascript"
  ],
  "author": "Gil Tayar <gil.tayar@applitools.com>",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://github.com/applitools/rendering-grid",
    "type": "git"
  },
  "dependencies": {
    "fastify": "^2.3.0",
    "@applitools/fastify-request-id": "^1.0.7",
    "@applitools/fastify-request-logger": "^3.0.0",
    "@applitools/loggly-pino": "^3.0.3"
  },
  "devDependencies": {
    "@applitools/docker-compose-mocha": "github:applitools/docker-compose-mocha",
    "@applitools/docker-compose-testkit": "^1.1.0",
    "@applitools/http-commons": "^1.2.9",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha-no-only": "^1.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^6.1.4",
    "mocha-parallel-tests": "^2.1.0",
    "node-fetch": "^2.4.1",
    "prettier": "^1.17.0"
  }
}
