{
  "name": "@applitools/templatetemplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/templatetemplate.js",
  "engines": {
    "node": ">=12.14.1"
  },
  "files": [
    "src",
    "scripts"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "bin": {
    "run-templatetemplate": "scripts/run-templatetemplate.js"
  },
  "scripts": {
    "start": "scripts/run-templatetemplate.js",
    "test": "npm run eslint && npm run test:mocha-parallel",
    "test:mocha": "mocha --no-timeouts --trace-warnings --exit 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "test:mocha-parallel": "mocha-parallel-tests --no-timeouts --trace-warnings --exit 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "eslint": "eslint '**/*.js'",
    "build": "#",
    "postbuild": "npm run build:docker",
    "build:docker": "docker build -t applitools/templatetemplate --build-arg NPM_FILE=`cat ~/.npmrc` . && docker tag applitools/templatetemplate applitools/templatetemplate:${npm_package_version}",
    "postpublish": "npm run publish:docker",
    "publish:docker": "docker push applitools/templatetemplate:${npm_package_version}"
  },
  "keywords": [
    "applitools",
    "nodejs",
    "javascript"
  ],
  "author": "Gil Tayar <gil.tayar@applitools.com>",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://github.com/applitools/rendering-grid",
    "type": "git",
    "directory": "packages/templatetemplate"
  },
  "dependencies": {
    "@applitools/loggly-pino": "^3.1.15",
    "fastify": "^2.13.0"
  },
  "devDependencies": {
    "@applitools/docker-compose-mocha": "^1.4.3",
    "@applitools/docker-compose-testkit": "^1.1.2",
    "@applitools/http-commons": "^2.4.3",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.1",
    "mocha-parallel-tests": "^2.3.0",
    "prettier": "^2.0.2"
  }
}
