    "directory": "packages/functional-commons"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.12",
    "chai": "^5.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-mocha-no-only": "^1.2.0",
