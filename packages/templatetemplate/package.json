    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@types/pg": "^7.14.7",
    "@types/yargs": "^15.0.10",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "eslint": "^7.14.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
